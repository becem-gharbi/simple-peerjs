{
  "name": "@bg-dev/simple-peerjs",
  "version": "0.1.2",
  "type": "module",
  "description": "Simple Peerjs",
  "author": "Becem Gharbi",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "peerjs",
    "webRTC",
    "chat",
    "realtime"
  ],
  "repository": {
    "url": "https://github.com/becem-gharbi/simple-peerjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "peer": "node peer/index.mjs",
    "dev": "cd playground & vite",
    "build": "rimraf dist && tsup src/**/*.ts --format esm --dts",
    "release": "npm run build && changelogen --release && npm publish && git push --follow-tags"
  },
  "devDependencies": {
    "changelogen": "^0.5.5",
    "peer": "^1.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "hookable": "^5.5.3",
    "peerjs": "^1.5.2"
  }
}